{
  "version": 2,
  "builds": [
    {
      "src": "dist/main.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/main.js"
    }
  ],
  "build": {
    "env": {
      "DATABASE_URL": "postgresql://postgres:12345678@localhost:5432/movie-reservation-system?schema=public"
    },
    "buildCommand": "npm run build && npx prisma generate"
  }
}
